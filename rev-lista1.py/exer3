n1 = int(input())
n2 = int(input())
n3 = int(input())
n4 = int(input())
s = 0
s2 = 0
if n1 % 2 == 0:
    s += n1
if n2 % 2 == 0:
    s += n2
if n3 % 2 == 0:
    s += n3
if n4 % 2 == 0:
    s += n4
print(F'Soma dos números pares = {s}')
if n1 % 2 != 0:
    s2 += n1
if n2 % 2 != 0:
    s2 += n2
if n3 % 2 != 0:
    s2 += n3
if n4 % 2 != 0:
    s2 += n4
print(F'Soma dos números impares = {s2}')
