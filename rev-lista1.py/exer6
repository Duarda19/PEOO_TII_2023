n1 = int(input())
n2 = int(input())
n3 = int(input())
maior = -1111111111111111
menor = 111111111111111111
if n1 > n2 and n1 > n3:
  maior = n1
if n2 > n1 and n2 > n3:
  maior = n2
if n3 > n2 and n3 > n1:
  maior = n3
if n1 < n2 and n1 < n3:
  menor = n1
if n2 < n1 and n2 < n3:
  menor = n2
if n3 < n2 and n3 < n1:
  menor = n3
soma = maior + menor
print(f'A soma do maior com o menor número é: {soma}')
